{
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "OWID table metadata",
    "description": "Metadata about an entire table of indicators and entities.",
    "type":"object",
    "required": ["slug"],
    "properties": {
        "name": {"type": "string", "pattern": "^[a-z0-9_]+$"},
        "title": {"type":"string"},
        "description": {"type": "string"}
    },
    "additionalProperties": false
}